<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.cellTable {
			width: 90%;
			heigth: 80%;
			border: 1px solid #ccc;
			text-align: left;
		}
	</ui:style>

	<g:HTMLPanel>
		<table width="100%">
			<tr>
				<td valign='top'>
					<c:CellTable addStyleNames='{style.cellTable}'

						ui:field='cellTable' />
				</td>
			</tr>
			<tr>
				<td align='center'>
					<c:SimplePager ui:field='pager' />
				</td>
			</tr>
			<tr>
				<td align='center'>

					<g:DecoratorPanel>
						<g:HorizontalPanel styleName="aucentre" spacing="10">
							<g:Button width="107px" text="Modifier" ui:field="modifyButton" />
							<g:Button text="Supprimer" ui:field='deleteButton' />
						</g:HorizontalPanel>
					</g:DecoratorPanel>
				</td>
			</tr>
		</table>


	</g:HTMLPanel>
</ui:UiBinder> 