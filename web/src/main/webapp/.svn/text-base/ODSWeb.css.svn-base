 
body {
    background: none repeat scroll 0 0 #666666;
    color: #000000;
    font: 100% Verdana,Arial,Helvetica,sans-serif;
    margin: 0;
    padding: 0;
    text-align: center;
}
.twoColHybLtHdr #container {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #000000;
    margin: 0 auto;
    text-align: left;
    width: 88%;
}
.twoColHybLtHdr #header {
    background: none repeat scroll 0 0 #FFFFF;
    padding: 0 0px;
    overflow : hidden; 
}

.twoColHybLtHdr #menu {
    background: none repeat scroll 0 0 #EBEBEB;
    float: left;
    padding: 15px 0;
    width: 12em;
    height: 42em;
    border:10px;
    border-color: black;
}

.twoColHybLtHdr #mainContent {
    margin: 0 20px 0 13em;
    height: 42em;
}
.twoColHybLtHdr #footer {
    background: none repeat scroll 0 0 #DDDDDD;
    padding: 0 10px;
}
.twoColHybLtHdr #footer p {
    margin: 0;
    padding: 10px 0;
}
.fltrt {
    float: right;
    margin-left: 8px;
}
.fltlft {
    float: left;
    margin-right: 8px;
}
.clearfloat {
    clear: both;
    font-size: 1px;
    height: 0;
    line-height: 0;
}
.cw-StackPanelHeader {
font-family: fantasy;
font-size: xx-small;
	}

.fieldods {
	height : 20px;
	border: #0000FF;
	font-size: 36px;
}
